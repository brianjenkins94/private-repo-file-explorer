{
  "name": "private-repo-file-explorer",
  "version": "1.0.0",
  "description": "Share a private repo as if it were a public repo.",
  "bin": "bootstrap",
  "scripts": {
    "serve": "nodemon --watch \"**/*.ts\" --exec ts-node server.ts",
    "tree": "find . -type f -not -path \"./.git/**\" -not -path \"./node_modules/**\" > tree.txt",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brianjenkins94/private-repo-file-explorer.git"
  },
  "author": "Brian Jenkins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brianjenkins94/private-repo-file-explorer/issues"
  },
  "homepage": "https://github.com/brianjenkins94/private-repo-file-explorer#readme",
  "dependencies": {
    "@types/node": "latest",
    "ejs": "latest",
    "express": "latest",
    "helmet": "latest",
    "morgan": "latest",
    "nodemon": "latest",
    "open": "latest",
    "portfinder": "latest",
    "ts-node": "latest",
    "typescript": "latest"
  },
  "devDependencies": {
    "@types/ejs": "latest",
    "@types/express": "latest",
    "@types/morgan": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest"
  }
}
